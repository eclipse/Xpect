/* XPECT_SETUP org.eclipse.xpect.xtext.lib.tests.util.grammaranalyzer.GrammarAnalyzerTest END_SETUP  */
grammar foo.Bar hidden(SL_COMMENT1, SL_COMMENT2, SL_COMMENT3)

generate foo "http://foo/bar"
import "http://www.eclipse.org/emf/2002/Ecore" as ecore

/* XPECT comments ---
	SLCommentRule: '#' -> '\n'
	SLCommentRule: '$' -> '\n'
	SLCommentRule: '%$' -> '\n'
	SLCommentRule: '%@' -> '\n'
	SLCommentRule: '@' -> '\n'
 --- */
Rule:
	name="foo";

terminal SL_COMMENT1:
	'#' !('\r' | '\n') ('\r' | '\n' | EOF);

terminal SL_COMMENT2:
	('@' | '$') !('\r' | '\n') ('\r' | '\n' | EOF);

terminal SL_COMMENT3:
	'%' ('@' | '$') !('\r' | '\n') ('\r' | '\n' | EOF);
